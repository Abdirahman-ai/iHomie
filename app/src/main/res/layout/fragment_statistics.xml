<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="823dp"
    android:background="@drawable/background_color"
    tools:context=".StatisticsFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="-1dp"
        android:layout_gravity="top|left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Statistics"
        android:textColor="#FF000000"
        android:textSize="36dp" />

    <ImageView
        android:layout_width="367dp"
        android:layout_height="52dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="232dp"
        android:layout_gravity="top|left"
        android:background="@drawable/textbackground_color"
        android:scaleType="centerInside" />

    <ImageView
        android:id="@+id/iv_search"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="245dp"
        android:layout_gravity="top|left"
        android:alpha="0.699999988079071"
        android:scaleType="centerInside"
        android:src="@drawable/search_alt_light" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="236.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="top|left"
        android:alpha="0.6000000238418579"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:hint="500 Tech Way, Palo Alto, CA 94020"
        android:textColor="#FF000000"
        android:textSize="20dp" />

    <FrameLayout
        android:id="@+id/container_group"
        android:layout_width="367dp"
        android:layout_height="82dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="128dp"
        android:layout_gravity="top|left">

        <FrameLayout
            android:id="@+id/container_destination_instruct"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/image_rectangle"
                android:layout_width="367dp"
                android:layout_height="82dp"
                android:layout_gravity="center"
                android:background="@drawable/pinkrectangle"
                android:scaleType="centerInside" />

            <TextView
                android:id="@+id/text_enter_destination"
                android:layout_width="269dp"
                android:layout_height="47dp"
                android:layout_marginLeft="68dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="18dp"
                android:layout_marginBottom="17dp"
                android:alpha="0.6000000238418579"
                android:gravity="left|top"
                android:includeFontPadding="false"
                android:lineHeight="25.03dp"
                android:text="Enter destination address to compare commute time"
                android:textColor="#FF000000"
                android:textSize="21dp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/container_road"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="23dp"
            android:layout_marginBottom="17dp"
            android:layout_gravity="left"
            android:scaleType="centerInside"
            android:src="@drawable/road" />

    </FrameLayout>

</FrameLayout>